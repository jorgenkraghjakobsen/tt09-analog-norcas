// Register database generic build system
sys_cfg.enable_stuf 0x00 0 1 0 Enable stuf
sys_cfg.enable_other 0x00 1 1 1 Enable other stuf
sys_cfg.monitor_flag 0x00 2 1 0 Monitor internal flag
sys_cfg.pwm_duty 0x01 0 8 133 Counter value for pwm
sys_cfg.debug_led 0x02 0 6 254 Debug led signals
sys_cfg.spare_0 0x03 0 8 17 Spare_reg0
sys_cfg.spare_1 0x04 0 8 34 Spare_reg1
sys_cfg.spare_2 0x05 0 8 51 Spare_reg2
